@model List<WebApplication2.Models.ApplicationUser>
@{
    ViewBag.Title = "UserList";
}
<div class="container">
    <h2>UserList</h2>
    <table class="table">
        <thead>
            <tr>
                <th>UserName</th>
                <th>FirtName</th>
                <th>LastName</th>
                <th>Phone</th>
                <th>Address</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.UserName</td>
                    <td>@item.firstName</td>
                    <td>@item.lastName</td>
                    <td>@item.Phone</td>
                    <td>@item.Address</td>
                </tr>
            }
        </tbody>
    </table>
</div>
